{
  "name": "BeMoreOriginal",
  "description": "Stops you from posting unoriginal tweets",
  "version": "0.4",
  "permissions": ["activeTab", "http://agermanidis.com:5141/*"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["style.css"],
      "js": ["jquery.js", "index.js"]
    }
  ],
  "background": {
    "scripts": ["jquery.js", "background.js"]
  },
  "icons": {
    "16": "images/bemoreoriginal_16x16.png",
    "48": "images/bemoreoriginal_48x48.png",
    "128": "images/bemoreoriginal_128x128.png"
  },
  "web_accessible_resources": [
    "images/loader.gif",
    "images/success.png",
    "images/fail.png"
  ],
  "manifest_version": 2
}
